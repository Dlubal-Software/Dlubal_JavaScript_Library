{
  "metadata": [
    {
      "src": [
      //   {
      //     "files": [
      //       "src/**.csproj"
      //     ]
      //   }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"//,
          //"api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_enableSearch": true,
      "_appTitle": "Dlubal JavaScript High Level Functions Library",
      "_appFaviconPath": "images/favicon.ico",
      "_appLogoPath": "images/logo.png",
      "_appFooter": "Copyright Â© 2022 <a href=\"https://www.dlubal.com/en-US\">Dlubal Software</a><br>Generated by <strong>DocFX</strong>",
      "_gitContribute": {
          "repo": "https://github.com/Dlubal-Software/Dlubal_JavaScript_Library.git",
          "branch": "main",
          "path": "Docs/"
      },
      "_gitUrlPattern": "github"
  },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material/material"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}